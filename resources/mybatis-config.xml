    <!-- CONFIG ALIASES -->
    <typeAliases>
    	<typeAlias alias="PptpdAdmin" type="com.org.c2y2.entity.PptpdAdmin"/>
    	<typeAlias alias="PptpdUser" type="com.org.c2y2.entity.PptpdUser"/>
    </typeAliases>
</configuration>
